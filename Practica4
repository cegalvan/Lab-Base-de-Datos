	values('10','Direccion10','Lugar2')

insert into Arena (idArena,Direccion,NombreArena)
	values('11','Direccion11','LugarInvalido')


update Arena set NombreArena = 'Arena1'
where idArena= '10'

delete from Arena
	where idArena= '11'


